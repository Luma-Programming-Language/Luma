@module "main"

@use "std_args" as args
@use "std_io" as io 

pub const main -> fn (argc: int, argv: **byte) int {
  let a: Args = args::init_args(argc, argv);
  a = a.tail();

  let it: ArgsIter = a.iter();
  loop {
    let arg: *byte = it.next();
    if (arg == cast<*byte>(0)) break;
    io::print("%s\n", [io::str_arg(arg)]);
  }

  return 0;
}
